<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.zss.prolist.title_bar.TitleBar
        android:id="@+id/titleCustom"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:leftBackground="@android:color/holo_blue_dark"
        app:leftText="Back"
        app:leftTextColor="#FFFFFF"
        app:rightBackground="@android:color/holo_blue_dark"
        app:rightText="More"
        app:rightTextColor="#FFFFFF"
        app:titleText="自定义标题"
        app:titleTextColor="#123412"
        app:titleTextSize="10sp" />

</LinearLayout>